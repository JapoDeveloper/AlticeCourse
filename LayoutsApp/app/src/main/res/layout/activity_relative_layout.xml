<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:padding="10dp"
    tools:context="co.japo.layoutsapp.RelativeLayoutActivity">

    <ImageView
        android:id="@+id/amazon"
        android:layout_width="64dp"
        android:layout_height="64dp"
        android:layout_below="@+id/steam"
        android:layout_alignLeft="@+id/steam"
        android:src="@drawable/amazon"/>

    <!--<ImageView-->
        <!--android:id="@+id/app_store"-->
        <!--android:layout_width="64dp"-->
        <!--android:layout_height="64dp"-->
        <!--android:layout_alignParentTop="true"-->
        <!--android:layout_alignParentRight="true"-->
        <!--android:src="@drawable/app_store"/>-->

    <ImageView
        android:id="@+id/dropbox"
        android:layout_width="64dp"
        android:layout_height="64dp"
        android:layout_toLeftOf="@+id/stackoverflow"
        android:layout_below="@+id/github"
        android:src="@drawable/dropbox"/>

    <ImageView
        android:id="@+id/evernote"
        android:layout_width="64dp"
        android:layout_height="64dp"
        android:layout_below="@+id/twitter"
        android:layout_toLeftOf="@+id/twitter"
        android:src="@drawable/evernote"/>

    <ImageView
        android:id="@+id/facebook"
        android:layout_width="64dp"
        android:layout_height="64dp"
        android:layout_toRightOf="@+id/twitter"
        android:layout_below="@+id/reddit"
        android:src="@drawable/facebook"/>

    <ImageView
        android:id="@+id/github"
        android:layout_width="64dp"
        android:layout_height="64dp"
        android:layout_centerHorizontal="true"
        android:layout_alignParentTop="true"
        android:src="@drawable/github"/>

    <ImageView
        android:id="@+id/google_drive"
        android:layout_width="64dp"
        android:layout_height="64dp"
        android:layout_toRightOf="@+id/wordpress"
        android:layout_below="@+id/github"
        android:src="@drawable/google_drive"/>

    <ImageView
        android:id="@+id/google_plus"
        android:layout_width="64dp"
        android:layout_height="64dp"
        android:layout_above="@+id/itunnes"
        android:layout_centerHorizontal="true"
        android:src="@drawable/google_plus"/>

    <ImageView
        android:id="@+id/instagram"
        android:layout_width="64dp"
        android:layout_height="64dp"
        android:layout_toRightOf="@+id/facebook"
        android:layout_below="@+id/whatsapp"
        android:src="@drawable/instagram"/>

    <ImageView
        android:id="@+id/itunnes"
        android:layout_width="64dp"
        android:layout_height="64dp"
        android:layout_above="@+id/pinterest"
        android:layout_centerHorizontal="true"
        android:src="@drawable/itunnes"/>

    <ImageView
        android:id="@+id/linkedin"
        android:layout_width="64dp"
        android:layout_height="64dp"
        android:layout_toRightOf="@+id/stackoverflow"
        android:layout_below="@id/github"
        android:src="@drawable/linkedin"/>

    <ImageView
        android:id="@+id/pinterest"
        android:layout_width="64dp"
        android:layout_height="64dp"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:src="@drawable/pinterest"/>

    <ImageView
        android:id="@+id/play_store"
        android:layout_width="64dp"
        android:layout_height="64dp"
        android:layout_below="@id/amazon"
        android:layout_alignLeft="@+id/amazon"
        android:src="@drawable/play_store"/>

    <ImageView
        android:id="@+id/playstation"
        android:layout_width="64dp"
        android:layout_height="64dp"
        android:layout_below="@+id/sound_cloud"
        android:layout_alignLeft="@+id/sound_cloud"
        android:src="@drawable/playstation"/>

    <ImageView
        android:id="@+id/reddit"
        android:layout_width="64dp"
        android:layout_height="64dp"
        android:layout_toLeftOf="@+id/stackoverflow"
        android:layout_below="@+id/stackoverflow"
        android:src="@drawable/reddit"/>

    <ImageView
        android:id="@+id/sound_cloud"
        android:layout_width="64dp"
        android:layout_height="64dp"
        android:layout_below="@+id/evernote"
        android:layout_alignLeft="@+id/evernote"
        android:src="@drawable/sound_cloud"/>

    <ImageView
        android:id="@+id/stackoverflow"
        android:layout_width="64dp"
        android:layout_height="64dp"
        android:layout_below="@+id/github"
        android:layout_toLeftOf="@+id/github"
        android:src="@drawable/stackoverflow"/>

    <ImageView
        android:id="@+id/steam"
        android:layout_width="64dp"
        android:layout_height="64dp"
        android:layout_below="@+id/instagram"
        android:layout_toRightOf="@+id/instagram"
        android:src="@drawable/steam"/>

    <ImageView
        android:id="@+id/twitter"
        android:layout_width="64dp"
        android:layout_height="64dp"
        android:layout_below="@+id/reddit"
        android:layout_toRightOf="@+id/reddit"
        android:src="@drawable/twitter"/>

    <ImageView
        android:id="@+id/whatsapp"
        android:layout_width="64dp"
        android:layout_height="64dp"
        android:layout_toRightOf="@+id/wordpress"
        android:layout_below="@+id/wordpress"
        android:src="@drawable/whatsapp"/>

    <ImageView
        android:id="@+id/wordpress"
        android:layout_width="64dp"
        android:layout_height="64dp"
        android:layout_below="@+id/github"
        android:layout_toRightOf="@+id/linkedin"
        android:src="@drawable/wordpress"/>
</RelativeLayout>
